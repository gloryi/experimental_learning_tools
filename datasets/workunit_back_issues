        resp = {"process_id": request.session.get('process_id'),
                "production_status": request.session.get('production_status')}
        return Response(resp, status=status.HTTP_200_OK)

    @logger.func_log(level=logging.INFO, name=logname, klass=name)
    def put(self, request: HttpRequest) -> Response:
        return Response({}, status=status.HTTP_403_FORBIDDEN)

    @logger.func_log(level=logging.INFO, name=logname, klass=name)
    def delete(self, request: HttpRequest) -> Response:
        return Response({}, status=status.HTTP_403_FORBIDDEN)


@permission_classes((permissions.AllowAny,))
class NestingView(APIView):
    """API View class for start nesting page
    (url: api/v1/operations/nesting/)."""

    name = 'NestingView'

    # start nesting operation
    @logger.func_log(level=logging.INFO, name=logname, klass=name)
    def get(self, request: HttpRequest, *args, **kwargs) -> Response:
        """Processing get request.

        Get details list for work with given leather.
        Start nesting process. Return json with details
        on the skin or json with error.

        Args:
            request: Http get request.

        Returns:
            Json with details on the skin or json with error.

        """

        if request.session.get('nesting_id'):
            del request.session['nesting_id']

        logger.log_msg(level=logging.DEBUG,
                       name=logname,
                       msg=f"del request.session['nesting_id']")

        if settings.TEST_DATA:

            details_id = 35

            logger.log_msg(level=logging.DEBUG,
                           name=logname,
                           msg=f"Run {self.name} in test mode")

            defects_id = 1


        else:

            leather_data = request.session.get('leather_material')
            if not leather_data:
                logger.log_msg(level=logging.ERROR,
                               name=logname,
                               msg=f"LEATHER MATERIAL IS EMPTY!")

                return Response({'param error': 'leather material is empty, lost'},
                                status=status.HTTP_500_INTERNAL_SERVER_ERROR)

            logger.log_msg(name=logname,
                           msg=f"Leather material is full: {leather_data}")

            # details request
            logger.log_msg(name=logname,
                           msg=f"New nesting request\n")
            logger.log_msg(name=logname,
                           msg=f"Try to init WUConnectionGRPCClient.")

            wu_grpc_client = WUConnectionGRPCClient()

            logger.log_msg(name=logname,
                           msg=f"Init WUConnectionGRPCClient success.")
            defect_id = request.session.get('defect_id')
            details_id = \
                DetailsRequestToModelSerializer(
                    leather_data=leather_data,
                    defect_data=Defects.objects.get(id=defect_id).grpc_data).serialize()

            try:
                logger.log_msg(name=logname,
                               msg=f"Try to send gRPC request to "
                                   f"WUConnectionGRPCClient.get_details.\n"
                                   f"details_id={details_id}")
                details = Details.objects.get(id=details_id).grpc_request
                details = json.loads(details)
                details_response = wu_grpc_client.get_detail_list(details)

                with open(os.path.join(settings.BASE_DIR, 'jsons',
                                       'details_response.json'), 'w') as f:
                    json.dump(details_response, f,
                              indent=4, sort_keys=True)

            except GrpcError as e:

                logger.log_msg(level=logging.ERROR,
                               name=logname,
                               msg=f"gRPC request error:"
                                   f"{e}")

                return Response(e.get_message(),
                                status=status.HTTP_500_INTERNAL_SERVER_ERROR)

            logger.log_msg(name=logname,
                           msg=f"Try to send gRPC details response to "
                               f"DetailsResponseToModelSerializer. \n"
                               f"kwargs - details_id={details_id}")

            DetailsResponseToModelSerializer(
                details_id=details_id,
                details_data=details_response).serialize()

            defects_id = request.session.get('defect_id')

        # nesting request
        logger.log_msg(name=logname,
                       msg=f"Try to init NestingGRPCClient.")

        nesting_grpc_client = NestingGRPCClient()

        logger.log_msg(name=logname,
                       msg=f"Init NestingGRPCClient success.")

        nesting_id = NestingRequestToModelSerializer(
            defects_id=defects_id,  # request.session.get('defect_id'),
            details_id=details_id
        ).serialize()

        request.session['nesting_id'] = nesting_id
        request.session.modified = True

        try:
            logger.log_msg(name=logname,
                           msg=f"Try to send gRPC request to "
                               f"NestingGRPCClient.nesting. \n"
                               f"nesting object "
                               f"id = {nesting_id}")
            nest = Nesting.objects.get(id=nesting_id).grpc_request
            nest = json.loads(nest)
            nesting_grpc_response: dict = nesting_grpc_client.nesting(nest)
            with open(os.path.join(settings.BASE_DIR, 'jsons',
                                   'nesting_response.json'), 'w') as f:
                json.dump(nesting_grpc_response, f,
                          indent=4, sort_keys=True)

        except GrpcError as e:

            logger.log_msg(level=logging.ERROR,
                           name=logname,
                           msg=f"gRPC request error:"
                               f"{e}")

            return Response(e.get_message(),
                            status=status.HTTP_500_INTERNAL_SERVER_ERROR)
 (duplicate-code)
