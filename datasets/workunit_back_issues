@permission_classes((permissions.AllowAny,))
class ToProductionView(APIView):
    """API View class for
     page (url: api/v1/production/to_production/)."""

    name = 'ToProductionView'

    @logger.func_log(level=logging.INFO, name=logname, klass=name)
    def get(self, request: HttpRequest, *args, **kwargs) -> Response:
        """Processing get request.

        Start production process. Create request with given data, receive response with status

        Args:
            request: Http get request.
            *args:
            **kwargs:

        Returns:
            Json with production status or json with error.
        """

        debug_flag = False
        # 0 - false, 1 - true
        if request.query_params.get('debug_flag') and \
                str(request.query_params.get('debug_flag')) == '1':
            #  must number details
            logger.log_msg(name=logname,
                           msg=f"They want to number details.")
            debug_flag = True

        logger.log_msg(name=logname,
                       msg=f"Try to init WUGRPCClient.")

        wu_grpc_client = WUConnectionGRPCClient()

        logger.log_msg(name=logname,
                       msg=f"Init WUGRPCClient success.")

        if request.session.get('prod_id'):
            del request.session['prod_id']

        logger.log_msg(name=logname,
                       msg=f"del request.session['prod_id']")

        if settings.TEST_DATA:
            prod_id = ToProductionRequestToModelSerializer(
                defects_id=1,  # request.session.get('defect_id'),
                nesting_id=request.session.get('nesting_id'),
            ).serialize()
            request.session['prod_id'] = prod_id

            request.session['production_status'] = 'ready'
            request.session.modified = True
            time.sleep(20)
            response = {'process_id': request.session.get('process_id'),
                        'production_status':
                            request.session.get('production_status')}
            return Response(response, status=status.HTTP_200_OK)
        else:

            prod_id = ToProductionRequestToModelSerializer(
                defects_id=request.session.get('defect_id'),
                nesting_id=request.session.get('nesting_id'),
                debug_flag=debug_flag
            ).serialize()

            # First check we have an id session already

            request.session['prod_id'] = prod_id
            request.session['production_status'] = 'ready'
            request.session.modified = True

            logger.log_msg(name=logname,
                           msg=f"Create new "
                               f"request.session['prod_id'] = {prod_id}")

            try:
                logger.log_msg(name=logname,
                               msg=f"Try to send gRPC request to "
                                   f"WUConnectionGRPCClient."
                                   f"run_to_production.\n"
                                   f"prod_id={prod_id}")
                prod = Production.objects.get(id=prod_id).grpc_request
                prod = json.loads(prod)
                prod_response = wu_grpc_client.run_to_production(prod)
                logger.log_msg(name=logname,
                               msg=f"ANSWER"
                                   f"{prod_response}")
                with open(os.path.join(settings.BASE_DIR, 'jsons',
                                       'prod_response.json'), 'w') as f:
                    json.dump(prod_response, f,
                              indent=4, sort_keys=True)

            except GrpcError as e:
                request.session['production_status'] = 'error'
                request.session.modified = True

                logger.log_msg(level=logging.ERROR,
                               name=logname,
                               msg=f"gRPC request error:"
                                   f"{e}")
                mess = e.get_message()['production_status'] = request.session.get('production_status')
                return Response(mess,
                                status=status.HTTP_500_INTERNAL_SERVER_ERROR)

            logger.log_msg(name=logname,
                           msg=f"Try to send gRPC to_production response to "
                               f"ToProductionResponseToModelSerializer. \n"
                               f"kwargs - prod_id={prod_id},"
                               f"production data get from "
                               f"production object {prod_id}")

            ToProductionResponseToModelSerializer(
                production_id=prod_id,
                production_data=prod_response).serialize()

            logger.log_msg(name=logname,
                           msg=f"Try to get serialized to_production data "
                               f"to "
                               f"ToProductionToJsonSerializer. \n"
                               f"kwargs - prod_id={prod_id}")

            prod_data = serializers.ToProductionToJsonSerializer(
                production_id=prod_id)

        with open(os.path.join(settings.BASE_DIR, 'jsons',
                               'prod-response.json'), 'w') as f:
            json.dump(prod_data.serialize(), f,
                      indent=4, sort_keys=True)
        response = prod_data.serialize()
        request.session['production_status'] = 'finished'
        request.session.modified = True
        response['process_id'] = request.session.get('process_id')
        response['production_status'] = \
            request.session.get('production_status')
        return Response(response, status=status.HTTP_200_OK)

    @logger.func_log(level=logging.INFO, name=logname, klass=name)
    def post(self, request: HttpRequest) -> Response:
        return Response({}, status=status.HTTP_403_FORBIDDEN)

    @logger.func_log(level=logging.INFO, name=logname, klass=name)
    def put(self, request: HttpRequest) -> Response:
        return Response({}, status=status.HTTP_403_FORBIDDEN)

    @logger.func_log(level=logging.INFO, name=logname, klass=name)
    def delete(self, request: HttpRequest) -> Response:
        return Response({}, status=status.HTTP_403_FORBIDDEN) (duplicate-code)
workunit_back/utils/__init__.py:1:0: R0801: Similar lines in 2 files
